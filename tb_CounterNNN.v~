`timscale 1ns/1ns

module tb_CounterNNN();

   /*AUTOREGINPUT*/
   /*AUTOWIRE*/

   CounterNNN TestCounter(/*AUTOINST*/);

   always #5 i_clk = ~i_clk;

   always @(negedge i_rstn) #1 i_rstn = 1;

   initial begin

      i_rstn = 0;

      #100;

      i_up = 1;

      #5000;

      i_down = 1;

      #100;

      i_up = 1;

      #5000;

      $finish;

   end // initial begin
endmodule // tb_CounterNNN

